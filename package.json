{
  "name": "your-secure-browser-app",
  "version": "1.0.0",
  "description": "A secure browser with profile management using Electron and Playwright.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "clean": "rm -r -fo node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "browser",
    "fingerprint",
    "privacy",
    "automation",
    "playwright"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@sparticuz/chromium-min": "^141.0.0",
    "electron": "^28.0.0",
    "electron-store": "^8.2.0",
    "fingerprint-generator": "^2.1.76",
    "node-fetch": "^2.6.7",
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.yourcompany.securebrowser",
    "productName": "Secure Browser Manager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  }
}